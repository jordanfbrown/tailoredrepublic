<div class="suit-wrapper three columns">
  <%= image_tag product.image_large_url %>
  <div class="suit-info-wrapper">
    <div class="suit-info">
      <div class="suit-name"><%= product.name %></div>
      <a href="#" class="customize">Click To Customize</a>
    </div>
    <div class="suit-price">
      <div class="suit-price-text-wrapper">
        <span class="dollar">$</span><span class="price"><%= product.display_price %></span>
      </div>
    </div>
  </div>
</div>