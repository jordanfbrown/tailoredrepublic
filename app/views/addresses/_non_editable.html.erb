<ul>
  <li><span class="label">Name:</span> <%= address.name %></li>
  <li><span class="label">Address Line 1:</span> <%= address.line1 %></li>
  <li><span class="label">Address Line 2:</span> <%= address.line2 %></li>
  <li><span class="label">City:</span> <%= address.city %></li>
  <li><span class="label">State:</span> <%= address.state %></li>
  <li><span class="label">Zip:</span> <%= address.zip %></li>
  <%= fields_for address do |address_fields| %>
    <%= address_fields.hidden_field :name  %>
    <%= address_fields.hidden_field :line1  %>
    <%= address_fields.hidden_field :line2  %>
    <%= address_fields.hidden_field :city  %>
    <%= address_fields.hidden_field :state  %>
    <%= address_fields.hidden_field :zip  %>
  <% end %>
</ul>